{
  "name": "pdfmastertool",
  "version": "3.0.0",
  "description": "43+ PDF tools - Better than iLovePDF - 100% FREE, Privacy-first, Ad-free, No-login",
  "type": "module",
  "scripts": {
    "dev": "astro dev --port 4321",
    "start": "astro dev --port 4321",
    "build": "cross-env NODE_ENV=production astro build",
    "preview": "cross-env NODE_ENV=production astro preview --port 4321 --host",
    "preview:compressed": "node preview-server.js",
    "astro": "astro",
    "check": "astro check --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "format": "prettier --write .",
    "deploy:aws": "cd aws && sh deploy.bat",
    "deploy:aws:bash": "cd aws && bash deploy.sh",
    "lighthouse": "lighthouse http://localhost:9001 --output=html,json --output-path=./lighthouse-report --chrome-flags=\"--headless\"",
    "lighthouse:all": "node scripts/lighthouse-test.js",
    "test:lighthouse": "npm run build && npm run preview:compressed & sleep 5 && npm run lighthouse && pkill -f preview-server"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.1",
    "@aws-sdk/client-lambda": "^3.645.0",
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "astro": "^5.0.0",
    "blob-util": "^2.0.2",
    "file-saver": "^2.0.5",
    "framer-motion": "^11.5.4",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "jszip": "^3.10.1",
    "mammoth": "^1.8.0",
    "openai": "^4.56.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.4.168",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.2.3",
    "tailwindcss": "^3.4.1",
    "tesseract.js": "^5.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^1.2.3",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.5.4"
  },
  "keywords": [
    "pdf",
    "pdf-tools",
    "ai-pdf",
    "pdf-converter",
    "pdf-editor",
    "privacy-first",
    "wasm",
    "astro",
    "react"
  ],
  "author": "PDFMasterTool",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  }
}
